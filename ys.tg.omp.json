{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"foreground": "white",
					"properties": {
						"fetch_version": false
					},
					"style": "plain",
					"template": "({{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }})",
					"type": "python"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "darkGray",
					"style": "plain",
					"template": "{{ .Icon }} |",
					"type": "os",
					"properties": {
						"windows": "<blue>WIN</>",
						"ubuntu": "<magenta>UBN</>"
					}
				},
				{
					"foreground": "green",
					"style": "plain",
					"template": " {{ if .Root }}<red>{{ end }}{{ .UserName }}{{ if .Root }}</>{{ end }}<darkGray>@</><cyan>{{ .HostName }}</>",
					"type": "session"
				},
				{
					"foreground": "lightYellow",
					"properties": {
						"style": "full"
					},
					"style": "plain",
					"template": "<darkGray>:</>{{ .Path }} ",
					"type": "path"
				},
				{
					"foreground": "#60ff60",
					"style": "plain",
					"template": "<darkGray>|</> Node {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
					"type": "node"
				},
				{
					"style": "plain",
					"template": " <darkGray>|</> <white>git:</><cyan>{{ .HEAD }}</> <lightMagenta>{{ .BranchStatus }}</>{{ if .Working.Changed }} <lightRed>≠</><red>{{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <lightGreen>↗</><green>{{ .Staging.String }}</>{{ end }}",
					"type": "git",
					"properties": {
						"fetch_status": true
					}
				},
				{
					"foreground": "red",
					"style": "plain",
					"template": " C:{{ if gt .Code 0 }}{{ .Code }}{{ end }} ",
					"type": "status"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "green",
					"style": "plain",
					"template": "{{ if .Root }}<red>{{ end }}${{ if .Root }}</>{{ end }}",
					"type": "text"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"version": 2
}